{
    "Kestrel": {
        "EndPoints": {
            "Http": {
                "Url": "http://localhost:3000"
            }
        }
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "AllowedHosts": "*",
    "CoreServer": {
        "AllowedFilesRegex": ".*\\.(json|dll)",
        "ServePath": "\\..\\",
        "IgnoreApplicationPath": true 
    }
}
